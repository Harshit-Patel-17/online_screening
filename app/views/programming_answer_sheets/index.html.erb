<div class="page-header text-center">
	<h1>Programming Answer Sheets</h1>
</div>
<div ng-controller="ProgrammingAnswerSheetIndexCtrl">
	<div class="form-inline">
		<select ng-model="programming_exam" class="form-control" ng-options="programming_exam.exam_name for programming_exam in programming_exams track by programming_exam.id">
			<option value="" style="display: none">Select Programming Exam</option>
		</select>
		<input type="number" class="form-control" placeholder="Cut-off score" ng-model="cut_off"></input>
		<button type="button" class="form-control" ng-click="getProgrammingAnswerSheets()">
			<i class="glyphicon glyphicon-search"></i>
		</button>
	</div><br>
	<div class="gridStyle" ng-grid="gridOptions">
	</div>
</div>