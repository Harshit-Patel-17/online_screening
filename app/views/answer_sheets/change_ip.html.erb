<div ng-controller="AnswerSheetChangeIpCtrl">
	<form class="form-inline">
		<input class="form-control" type="text" ng-model="params.first_name" placeholder="First name"></input>
		<input class="form-control" type="text" ng-model="params.email" placeholder="Email"></input>
		<input class="form-control" type="number" ng-model="params.college_id" placeholder="College Id"></input>
		<button class="btn btn-default" type="button" ng-click="getAnswerSheets()">
			<i class="glyphicon glyphicon-search"></i>
		</button>
		<button class="btn btn-default" type="reset">
			<i class="glyphicon glyphicon-remove"></i>
		</button>
	</form><br>
	<div class="gridStyle" ng-grid="gridOptions">
	</div>
</div>

<script type="text/ng-template" id="change_ip.html">
	<div class="modal-header">
	    <h3 class="modal-title">Enter New IP</h3>
	</div>
	<div class="modal-body">
	    <input type="text" ng-model="ip" placeholder="e.g. 192.168.1.1" class="form-control"></input>
	</div>
	<div class="modal-footer">
	    <button class="btn btn-primary" ng-click="ok()">OK</button>
	    <button class="btn btn-warning" ng-click="cancel()">Cancel</button>
	</div>
</script>
