<div class="page-header text-center">
	<h1>Answer Sheets</h1>
</div>
<div ng-controller="AnswerSheetIndexCtrl">
	<div class="form-inline">
		<select ng-model="exam" class="form-control" ng-options="exam.exam_name for exam in exams track by exam.id">
			<option value="" style="display: none">Select Exam</option>
		</select>
		<input type="number" class="form-control" placeholder="Cut-off score" ng-model="cut_off"></input>
		<button type="button" class="btn btn-default" ng-click="getAnswerSheets()">
			<i class="glyphicon glyphicon-search"></i>
		</button>
	</div><br>
	<div class="gridStyle" ng-grid="gridOptions">
	</div>
</div>