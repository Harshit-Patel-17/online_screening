<div ng-controller="AnswerSheetIndexCtrl">
	<select ng-model="exam" ng-options="exam.exam_name for exam in exams track by exam.id">
		<option value="" style="display: none">Select Exam</option>
	</select>
	<input type="number" placeholder="Cut-off score" ng-model="cut_off"></input>
	<button type="button" ng-click="getAnswerSheets()">
		<i class="glyphicon glyphicon-search"></i>
	</button>
	<div class="gridStyle" ng-grid="gridOptions">
	</div>
</div>